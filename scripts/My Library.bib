@article{langenMethodologiesSurveyingHerpetofauna2007,
  title = {Methodologies for Surveying Herpetofauna Mortality on Rural Highways},
  author = {Langen, Tom A. and Machniak, Angela and Crowe, Erin K. and Mangan, Charles and Marker, Daniel F. and Liddle, Neal and Roden, Brian},
