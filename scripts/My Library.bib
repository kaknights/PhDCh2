@article{kenningFieldEfficiencyBias2005,
  title = {Field Efficiency and Bias of Snag Inventory Methods},
  author = {Kenning, R. S. and Ducey, M. J. and Brissette, J. C. and Gove, J. H.},
  date = {2005-12},
  journaltitle = {Canadian Journal of Forest Research},
