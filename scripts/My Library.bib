  author = {Kaartinen, Harri and Hyyppä, Juha and Yu, Xiaowei and Vastaranta, Mikko and Hyyppä, Hannu and Kukko, Antero and Holopainen, Markus and Heipke, Christian and Hirschmugl, Manuela and Morsdorf, Felix and Næsset, Erik and Pitkänen, Juho and Popescu, Sorin and Solberg, Svein and Wolf, Bernd Michael and Wu, Jee-Cheng},
  date = {2012-03-30},
  journaltitle = {Remote Sensing},
