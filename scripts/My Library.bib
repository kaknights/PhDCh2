  author = {Khan, Md Nabiul Islam and Hijbeek, Renske and Berger, Uta and Koedam, Nico and Grueters, Uwe and Islam, S. M. Zahirul and Hasan, Md Asadul and Dahdouh-Guebas, Farid},
