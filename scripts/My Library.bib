  shorttitle = {Shortfalls and {{Solutions}} for {{Meeting National}} and {{Global Conservation Area Targets}}},
  author = {Butchart, Stuart H.M. and Clarke, Martin and Smith, Robert J. and Sykes, Rachel E. and Scharlemann, Jörn P.W. and Harfoot, Mike and Buchanan, Graeme M. and Angulo, Ariadne and Balmford, Andrew and Bertzky, Bastian and Brooks, Thomas M. and Carpenter, Kent E. and Comeros-Raynal, Mia T. and Cornell, John and Ficetola, G. Francesco and Fishpool, Lincoln D.C. and Fuller, Richard A. and Geldmann, Jonas and Harwell, Heather and Hilton-Taylor, Craig and Hoffmann, Michael and Joolia, Ackbar and Joppa, Lucas and Kingston, Naomi and May, Ian and Milam, Amy and Polidoro, Beth and Ralph, Gina and Richman, Nadia and Rondinini, Carlo and Segan, Daniel B. and Skolnik, Benjamin and Spalding, Mark D. and Stuart, Simon N. and Symes, Andy and Taylor, Joseph and Visconti, Piero and Watson, James E.M. and Wood, Louisa and Burgess, Neil D.},
  date = {2015-09},
  journaltitle = {Conservation Letters},
  volume = {8},
