}

@article{bauerAerialSurveysMonitor2015,
  title = {Aerial Surveys to Monitor Bluefin Tuna Abundance and Track Efficiency of Management Measures},
  author = {Bauer, Robert Klaus and Bonhommeau, Sylvain and Brisset, Blandine and Fromentin, Jean-Marc},
