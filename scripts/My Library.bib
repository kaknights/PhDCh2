@article{toddUsingDungEstimate2008,
  title = {Using Dung to Estimate Gorilla Density: {{Modeling}} Dung Production Rate},
  shorttitle = {Using Dung to Estimate Gorilla Density},
  author = {Todd, Angelique F. and Kuehl, Hjalmar S. and Cipolletta, Chloe and Walsh, Peter D.},
  date = {2008-04},
  journaltitle = {International Journal of Primatology},
  shortjournal = {Int. J. Primatol.},
  volume = {29},
  number = {2},
  pages = {549--563},
  issn = {0164-0291},
  doi = {10.1007/s10764-008-9247-8},
  abstract = {There is an urgent need for information on western gorilla population sizes and distribution to improve present and plan future conservation actions. Researchers traditionally have estimated gorilla densities on the basis of nest counts despite demonstrated variation in nest production and decay rates. The variation may lead to large biases in estimates of gorilla abundance. We investigated the use of an alternate index of gorilla abundance, via defecation data collected from habituated gorillas at Bai Hokou, Central African Republic. Our sample of 274/370 defecation events/dung piles produced a production rate of ca. 5 dung piles/d: comparable to previous estimates based on much smaller sample sizes. Heuristic models that failed to account for imperfect dung pile detection produced a lower defecation rate estimate than that of a maximum likelihood model that explicitly modeled detection probability. Generalized linear modeling (GLM) showed that dung pile production rate was strongly linked to rainfall, suggesting that failure to correct for seasonal variation in dung pile production rates could lead to substantial biases in gorilla abundance estimates. In our study, failing to distinguish between the number of defecation events and the number of dung piles produced would lead to a ca. 31\% overestimate of true gorilla numbers. The use of dung as an index of gorilla abundance shows potential, but more fieldwork and modeling on seasonal variation in dung production rates is required.},
  langid = {english},
  keywords = {defecation rate,dung,elephant numbers,forest,gorilla density,lowland gorillas,loxodonta-africana-cyclotis,model,ndoki national-park,northern congo,pan-troglodytes,population-density,seed dispersal,survey},
  annotation = {WOS:000255058600017},
  file = {C\:\\Users\\kaknights\\Zotero\\storage\\QNB8CBJT\\Todd et al 2008 Using dung to estimate gorilla density.pdf}
}

@article{tomasDistanceSamplingSurveys2021,
  title = {Distance Sampling Surveys Reveal 17 Million Vertebrates Directly Killed by the 2020’s Wildfires in the {{Pantanal}}, {{Brazil}}},
  author = {Tomas, Walfrido Moraes and Berlinck, Christian Niel and Chiaravalloti, Rafael Morais and Faggioni, Gabriel Paganini and Strüssmann, Christine and Libonati, Renata and Abrahão, Carlos Roberto and do Valle Alvarenga, Gabriela and de Faria Bacellar, Ana Elisa and de Queiroz Batista, Flávia Regina and Bornato, Thainan Silva and Camilo, André Restel and Castedo, Judite and Fernando, Adriana Maria Espinóza and de Freitas, Gabriel Oliveira and Garcia, Carolina Martins and Gonçalves, Henrique Santos and de Freitas Guilherme, Mariella Butti and Layme, Viviane Maria Guedes and Lustosa, Ana Paula Gomes and De Oliveira, Ailton Carneiro and da Rosa Oliveira, Maxwell and de Matos Martins Pereira, Alexandre and Rodrigues, Julia Abrantes and Semedo, Thiago Borges Fernandes and de Souza, Rafael Augusto Ducel and Tortato, Fernando Rodrigo and Viana, Diego Francis Passos and Vicente-Silva, Luciana and Morato, Ronaldo},
  options = {useprefix=true},
  date = {2021-12-16},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
  volume = {11},
  number = {1},
  pages = {23547},
  publisher = {{Nature Publishing Group}},
  issn = {2045-2322},
  doi = {10.1038/s41598-021-02844-5},
  url = {https://www.nature.com/articles/s41598-021-02844-5},
  urldate = {2022-01-09},
  abstract = {Anthropogenic factors have significantly influenced the frequency, duration, and intensity of meteorological drought in many regions of the globe, and the increased frequency of wildfires is among the most visible consequences of human-induced climate change. Despite the fire role in determining biodiversity outcomes in different ecosystems, wildfires can cause negative impacts on wildlife. We conducted ground surveys along line transects to estimate the first-order impact of the 2020 wildfires on vertebrates in the Pantanal wetland, Brazil. We adopted the distance sampling technique to estimate the densities and the number of dead vertebrates in the 39,030 square kilometers affected by fire. Our estimates indicate that at least 16.952 million vertebrates were killed immediately by the fires in the Pantanal, demonstrating the impact of such an event in wet savanna ecosystems. The Pantanal case also reminds us that the cumulative impact of widespread burning would be catastrophic, as fire recurrence may lead to the impoverishment of ecosystems and the disruption of their functioning. To overcome this unsustainable scenario, it is necessary to establish proper biomass fuel management to avoid cumulative impacts caused by fire over biodiversity and ecosystem services.},
  issue = {1},
  langid = {english},
  keywords = {Biodiversity,Fire ecology,Wetlands ecology},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Biodiversity;Fire ecology;Wetlands ecology Subject\_term\_id: biodiversity;fire-ecology;wetlands-ecology},
  file = {C\:\\Users\\kaknights\\Zotero\\storage\\5UC7VWKL\\Tomas et al. - 2021 - Distance sampling surveys reveal 17 million verteb.pdf;C\:\\Users\\kaknights\\Zotero\\storage\\2Z7BW45B\\s41598-021-02844-5.html}
}

