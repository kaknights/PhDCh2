  shorttitle = {Landscape-Scale Effects of Fire on Bird Assemblages},
  author = {Taylor, Rick S. and Watson, Simon J. and Nimmo, Dale G. and Kelly, Luke T. and Bennett, Andrew F. and Clarke, Michael F.},
