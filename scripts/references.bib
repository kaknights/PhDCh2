@article{doanWhichMethodsAre2003,
  title = {Which {{Methods Are Most Effective}} for {{Surveying Rain Forest Herpetofauna}}?},
  author = {Doan, Tiffany M.},
  date = {2003},
  journaltitle = {Journal of Herpetology},
  volume = {37},
  number = {1},
