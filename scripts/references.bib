@article{damgaardEstimatingMeanPlant2014,
  title = {Estimating Mean Plant Cover from Different Types of Cover Data: A Coherent Statistical Framework},
  shorttitle = {Estimating Mean Plant Cover from Different Types of Cover Data},
  author = {Damgaard, C.},
  date = {2014-02},
  journaltitle = {Ecosphere},
  volume = {5},
