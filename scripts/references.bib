  title = {An On-Ice Aerial Survey of the {{Kane Basin}} Polar Bear ({{Ursus}} Maritimus) Subpopulation},
  author = {Wiig, Oystein and Atkinson, Stephen N. and Born, Erik W. and Stapleton, Seth and Arnold, Todd and Dyck, Markus and Laidre, Kristin L. and Lunn, Nicholas J. and Regehr, Eric},
