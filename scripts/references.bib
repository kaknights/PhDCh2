  doi = {10.1111/j.1472-4642.2011.00842.x},
  url = {http://doi.wiley.com/10.1111/j.1472-4642.2011.00842.x},
  urldate = {2018-08-17},
  abstract = {Aim A common strategy for conserving biodiversity in fire-prone environments is to maintain a diversity of post-fire age classes at the landscape scale, under the assumption that ‘pyrodiversity begets biodiversity’. Another strategy is to maintain extensive areas of a particular seral state regarded as vital for the persistence of threatened species, under the assumption that this will also cater for the habitat needs of other species. We investigated the likely effects of these strategies on bird assemblages in tree mallee vegetation, characterized by multistemmed Eucalyptus species, where both strategies are currently employed.},
  langid = {english},
  file = {C\:\\Users\\kaknights\\Zotero\\storage\\G8KMTQ7F\\Taylor et al. - 2012 - Landscape-scale effects of fire on bird assemblage.pdf}
}

@article{taylorSynthesisingBushmeatResearch2015,
  title = {Synthesising Bushmeat Research Effort in {{West}} and {{Central Africa}}: {{A}} New Regional Database},
  shorttitle = {Synthesising Bushmeat Research Effort in {{West}} and {{Central Africa}}},
  author = {Taylor, G. and Scharlemann, J. P. W. and Rowcliffe, M. and Kümpel, N. and Harfoot, M. B. J. and Fa, J. E. and Melisch, R. and Milner-Gulland, E. J. and Bhagwat, S. and Abernethy, K. A. and Ajonina, A. S. and Albrechtsen, L. and Allebone-Webb, S. and Brown, E. and Brugiere, D. and Clark, C. and Colell, M. and Cowlishaw, G. and Crookes, D. and De Merode, E. and Dupain, J. and East, T. and Edderai, D. and Elkan, P. and Gill, D. and Greengrass, E. and Hodgkinson, C. and Ilambu, O. and Jeanmart, P. and Juste, J. and Linder, J. M. and Macdonald, D. W. and Noss, A. J. and Okorie, P. U. and Okouyi, V. J. J. and Pailler, S. and Poulsen, J. R. and Riddell, M. and Schleicher, J. and Schulte-Herbrüggen, B. and Starkey, M. and van Vliet, N. and Whitham, C. and Willcox, A. S. and Wilkie, D. S. and Wright, J. H. and Coad, L. M.},
  options = {useprefix=true},
  date = {2015-01-01},
  journaltitle = {Biological Conservation},
  shortjournal = {Biological Conservation},
  volume = {181},
