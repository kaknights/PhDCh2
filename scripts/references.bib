@article{whitingEstimatingWaterfowlDensities2009,
  title = {Estimating {{Waterfowl Densities}} in a {{Flooded Forest}}: {{A Comparison}} of {{Methods}}},
  author = {Whiting, Jr., R. Montague and Cornes, J. Paul},
  date = {2009},
  journaltitle = {SOUTHEASTERN NATURALIST},
  volume = {8},
