  journaltitle = {Journal of the Royal Statistical Society Series C-Applied Statistics},
  volume = {60},
  pages = {301--316},
  issn = {0035-9254},
  doi = {10.1111/j.1467-9876.2010.00748.x},
  abstract = {Antarctic krill is a key species in the Antarctic food web, an important prey item for marine predators and a commercial fishery resource. Although single-beam echo-sounders are commonly used to survey the species, multi-beam echo-sounders may be more efficient because they sample a larger volume of water. However, multi-beam echo-sounders may miss animals because they involve lower energy densities. We adapt distance sampling theory to deal with this and to estimate krill density and biomass from a multi-beam echo-sounder survey. The method provides a general means for estimating density and biomass from multi-beam echo-sounder data.},
  keywords = {Acoustic,Antarctic krill,biomass,Distance sampling,fisheries acoustics,Multi-beam echo-sounder,south-georgia},
  annotation = {WOS:000288017300009}
}

