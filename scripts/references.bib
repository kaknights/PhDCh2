  series = {Special {{Issue}}: {{Long-term}} Ecological Research},
  volume = {25},
  number = {10},
  pages = {547--550},
  issn = {0169-5347},
  doi = {10.1016/j.tree.2010.07.002},
  url = {http://www.sciencedirect.com/science/article/pii/S0169534710001576},
  urldate = {2018-01-29},
  abstract = {The gross under-resourcing of conservation endeavours has placed an increasing emphasis on spending accountability. Increased accountability has led to monitoring forming a central element of conservation programs. Although there is little doubt that information obtained from monitoring can improve management of biodiversity, the cost (in time and/or money) of gaining this knowledge is rarely considered when making decisions about allocation of resources to monitoring. We present a simple framework allowing managers and policy advisors to make decisions about when to invest in monitoring to improve management.},
