  author = {Laake, J. L. and Collier, B. A. and Morrison, M. L. and Wilkins, R. N.},
  date = {2011-09},
  journaltitle = {JOURNAL OF AGRICULTURAL BIOLOGICAL AND ENVIRONMENTAL STATISTICS},
