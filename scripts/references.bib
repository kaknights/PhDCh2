@article{chiokDistanceSamplingSurvey2021,
  title = {A Distance Sampling Survey of the {{Critically Endangered Straw-headed Bulbul}} {{{\emph{Pycnonotus}}}}{\emph{ Zeylanicus}} in {{Singapore}}},
  author = {Chiok, Wen Xuan and Ng, Elize Y. X. and Tang, Qian and Lee, Jessica G. H. and Rheindt, Frank E.},
