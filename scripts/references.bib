@article{mariagil-sanchezEvaluatingMethodsSurveying2017,
  title = {Evaluating Methods for Surveying the {{Endangered Cuvier}}'s Gazelle {{Gazella}} Cuvieri in Arid Landscapes},
  author = {Maria Gil-Sanchez, Jose and Javier Herrera-Sanchez, F. and Alvarez, Begona and Arredondo, Angel and Bautista, Jesus and Cancio, Inmaculada and Castillo, Salvador and Angel Diaz-Portero, Miguel and de Lucas, Jesus and McCain, Emil and Perez, Joaquin and Rodriguez-Siles, Javier and Manuel Saez, Juan and Martinez-Valderrama, Jaime and Valenzuela, Gerardo and Qninba, Abdeljebbar and Virgos, Emilio},
