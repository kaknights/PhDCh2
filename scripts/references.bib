@article{nielsonOptimizationOrderedDistance2004,
  title = {Optimization of Ordered Distance Sampling},
  author = {Nielson, Ryan M. and Sugihara, Robert T. and Boardman, Thomas J. and Engeman, Richard M.},
  date = {2004-03},
  journaltitle = {Environmetrics},
